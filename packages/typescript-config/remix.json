{
  "extends": "./base.json",
  "compilerOptions": {
    "types": ["@remix-run/node", "vite/client"],
    "target": "ES2022",
    "jsx": "react-jsx",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "sourceMap": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmit": true
  }
}
